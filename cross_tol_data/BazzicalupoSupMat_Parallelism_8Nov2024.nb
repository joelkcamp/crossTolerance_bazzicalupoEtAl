     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"Product", "[", 
         RowBox[{
          FractionBox[
           RowBox[{"6607", "-", 
            RowBox[{"(", 
             RowBox[{"i", "-", "1"}], ")"}]}], "6607"], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"j", "-", "1"}]}], "}"}]}], "]"}], "*", 
        RowBox[{"(", 
         FractionBox[
          RowBox[{"j", "-", "1"}], "6607"], ")"}], "*", 
        RowBox[{"Product", "[", 
         RowBox[{
          FractionBox[
           RowBox[{"6607", "-", 
            RowBox[{"(", 
